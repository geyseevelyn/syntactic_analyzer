package CoreDatatypes

datatype AddressDataType {
	street: string
	city: string
	postalCode: string
	country: string
}

datatype PhoneNumberDataType {
	countryCode: intDataType [1]
	bodyNumber: intDataType [1]
}

datatype intDataType specializes number

enum EyeColorEnum {
	Blue,
	Green,
	Brown,
	Black
}